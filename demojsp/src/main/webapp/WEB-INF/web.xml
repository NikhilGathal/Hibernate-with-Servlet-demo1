  <servlet-class>demojsp.FirstServlet</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  <servlet-name>firstservlet</servlet-name>
  <url-pattern>/first</url-pattern>
  </servlet-mapping>
  
